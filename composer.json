{
    "name": "asfop/hasone",
    "description": "在一对一关联情况下的数据缓存的一种尝试",
    "license": "MIT",
    "keywords": [
        "php","hasone"
    ],
    "authors": [
        {
            "name": "xxx",
            "email": "xxx@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">=7.1|>=8.0|>=8.1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Asfop\\HasOne\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asfop\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "4.0.x-dev"
    }
}
